{
  "name": "darthvadarr-nuxt",
  "private": true,
  "version": "0.8.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 3777 --host 0.0.0.0",
    "dev:docker": "nuxt dev --port 3777 --host 0.0.0.0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml up --build -d",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:restart": "docker-compose -f docker-compose.dev.yml restart",
    "docker:dev:restart:frontend": "docker-compose -f docker-compose.dev.yml restart darthvadarr-nuxt-dev",
    "docker:dev:restart:api": "docker-compose -f docker-compose.dev.yml restart seerrbridge",
    "docker:prod": "docker-compose -f docker-compose.nuxt.yml up -d",
    "docker:down": "docker-compose -f docker-compose.nuxt.yml down",
    "docker:logs": "docker-compose -f docker-compose.nuxt.yml logs -f"
  },
  "dependencies": {
    "@nuxt/icon": "^1.4.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/core": "^10.9.0",
    "date-fns": "^3.6.0",
    "fernet": "^0.4.0",
    "mysql2": "^3.9.4",
    "nuxt": "^4.1.3",
    "pinia": "^2.1.7",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.69",
    "@nuxt/eslint-config": "^0.5.0",
    "@types/node": "^20.11.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vue-tsc": "^3.1.1"
  }
}
